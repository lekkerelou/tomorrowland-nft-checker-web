{
  "name": "tomorrowland-medallion-checker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore",
    "mint": "npx ts-node script/mint.ts",
    "snapshot": "npx ts-node script/snapshot.ts",
    "hold": "npx ts-node script/hold.ts",
    "full": "npm run mint && npm run snapshot && npm run hold",
    "cron": "npx ts-node script/cron.ts"
  },
  "dependencies": {
    "@solana/web3.js": "^1.70.1",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.2.1",
    "bottleneck": "^2.19.5",
    "chalk": "^4.1.2",
    "chartjs-adapter-moment": "^1.0.1",
    "chartjs-plugin-annotation": "^2.1.1",
    "child_process": "^1.0.2",
    "cli-progress": "^3.11.2",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.3",
    "json-beautify": "^1.1.1",
    "json-format": "^1.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-flatpickr-component": "^11.0.1",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.12",
    "@types/node-cron": "^3.0.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
